<div class="container box">
 <div class="row mb-3 mt-3">
  <div class="col-md-3 text-center">
     <%= form_with url: public_customers_path ,method: :get do |f| %>
       <%= f.text_field :key %>
       <%= f.submit"検索",class:"btn btn-success" %>
     <% end %>
  </div>
  <div class="col-md-2">
     <%= link_to"検索クリア",public_customers_path,class:"btn btn-info" %>
  </div>
 </div>
 <div class="row">
  <div class="col-md-5">
     <table class="table table-borderless text-center" id = "index_relationship_btn">
       <%= render "public/relationships/index_relationship",customers: @customers,search: @search %>
     </table>
   </div>
 </div>
</div>