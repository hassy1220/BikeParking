<h3>コメント<%= park_area.comments.count %>件</h3>
    <table>
    <% park_area.comments.each do |list| %>
      <tr>
        <td><%= image_tag list.customer.get_bike_image(50,50) %></td>
        <td><%= list.comment %></td>
      </tr>
    <% end %>
    </table>
    <%= form_with model:  park_comment,url: public_park_comments_path(park_area.id),local: false do |f| %>
      <%= f.text_field :comment,rows: 5 %>
      <%= f.submit"送信" %>
    <% end %>