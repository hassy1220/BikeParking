<%- if controller_name != 'sessions' %>
  <%= link_to "登録済の方はこちら", new_session_path(resource_name),class:"btn btn-success" %>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規登録はこちらから", new_registration_path(resource_name),class:"btn btn-success" %>
<% end %>


